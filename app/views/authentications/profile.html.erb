<!DOCTYPE html>
<html>
  <head>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_importmap_tags %>
    <title>Login</title>
  </head>
  <body>
    <div class="center">
        <%- if @session %>
            <p><%- @session %></p>
        <%- else %>
            <p>No account</p>
        <%- end %>
    </div>
  </body>
</html>
